/* pages/Mycoach/index.wxss */
page{ background: #f5f5f5}
.Myco_pic,.Myco,.Myco_pic01,.bgg,.Myco01,.Myco_bt,.Myco_left,.fanwei{ float: left;}
.Myco_pic,.Myco,.bgg,.Myco01,.Myco_bt,.fanwei{ width: 100%;}
.fanwei,.Myco_pic{ position: relative}
.bgg{ position: absolute; top: 0}
.Myco{ position: relative;  z-index: 1;}
.Myco_pic,.bgg,.fanwei{ height: 400rpx;}
.Myco_pic{ filter: blur(4px);}
.fanwei{ overflow: hidden}
.Myco{ box-sizing: border-box;}
.Myco{ padding: 30rpx;}
.Myco{ background: #fff;}
.Myco_pic01{ border-radius: 50%; width: 130rpx; height: 130rpx; border: 6rpx solid #fff; margin-top: -94rpx;}
.bgg{ background: rgba(0, 0, 0, 0.3)}
.Myco01{ padding-left: 10rpx; margin-top: 10rpx;}
.weui-cell__bd {
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	flex: 1
}
.weui-cell__ft {
	text-align: right;
	color: #999
}
.weui-cell {
	padding: 15px 5px;
	position: relative;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	align-items: center;
  border-bottom: 2rpx solid #eee;
}
.weui-cell__ft_in-access {
	padding-right: 15px;
	position: relative
}
.weui-cell__ft_in-access:after {
	content: " ";
	display: inline-block;
	height: 6px;
	width: 6px;
	border-width: 4rpx 4rpx 0 0;
	border-color: #ccc;
	border-style: solid;
	-webkit-transform: matrix(.71, .71, -.71, .71, 0, 0);
	transform: matrix(.71, .71, -.71, .71, 0, 0);
	top: -2px;
	position: absolute;
	top: 50%;
	margin-top: -4px;
	right: 2px
}
.Myco_01{ margin-top: 30rpx;}
.Myco_right{ border: 2rpx solid #e9543c; color: #e9543c; border-radius: 8rpx; line-height: 28px; padding: 0 20rpx;}
.jour_left,.jour_pic01,.jour_nr,.jour_02,.jour_03,.jour_pic02,.jour_04{ float: left}
.jour_left,.jour_right,.jour_02,.jour_03{ width: 100%;}
.jour_right,.jour_01{ float: right;}
.jour_right{ width: 96%; margin: 2%;}
.jour_01{ width: 82%;}
.jour_01{ box-sizing: border-box;}
.jour_pic01{ width: 15px; height: 17px;}
.jour_nr,.jour_02{ color: #333}
.jour_04{ color: #999;}
.jour_02{ font-size: 17px;}
.jour_nr{ font-size: 16px;}
.jour_04{ font-size: 14px;}
.jour_pic01{ margin-right: 12rpx;}
.jour_right{ border-left: 2rpx solid #eee;}
.jour_01{ background: #fff; box-shadow: 0 0 5px 2px rgba(0,0,0,0.05); border: 2rpx solid #eee;}
.jour_01{ padding: 30rpx;}
.jour_01{ border-radius: 10rpx;}
.jour_pic02{ width: 11px; height: 13px; margin-top: 6rpx; margin-right: 12rpx;}
.jour_03{ margin-top: 30rpx;}
.jour_01{ margin-bottom:36rpx;}
.jour_01:first-child{ margin-top: -70rpx;}
.Myco_bt{ margin-bottom: 60rpx;}
