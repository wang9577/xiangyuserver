      <view class="weui-cell__ft weui-cell__ft_in-access" wx:if="{{j.grade===5}}">黑金卡会员</view>
      </view>
      </navigator>
    </swiper-item>
    <swiper-item >
     <navigator wx:for="{{coach2}}" wx:for-item="y" url="../Student/index?id={{y.id}}">
      <view class="weui-cell">
