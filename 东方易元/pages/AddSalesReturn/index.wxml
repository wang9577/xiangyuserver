<!--pages/AddReturn/index.wxml-->
<view class="weui-cell weui-cell_input">
  <view class="weui-cell__bd">
    <view class="weui-label">姓名</view>
  </view>
  <view class="weui-cell__ft weui-cell__ft_in-access"><input class="weui-input" placeholder="请输入" bindinput='names'/></view>
</view>
<view class="weui-cells__title">内容</view>
<view class="weui-cells weui-cells_after-title">
  <view class="weui-cell">
    <view class="weui-cell__bd">
      <textarea class="weui-textarea" placeholder="点击编辑" bindinput='count' />
    </view>
  </view>
</view>
<view class="weui-btn-area">
  <button class="weui-btn_warn" bindtap='save'>提交</button>
</view>