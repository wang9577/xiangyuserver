<!--pages/examination/index.wxml-->
<view class="weui-cell weui-cell_access">
  <view class="weui-cell__bd">体检时间</view>
  <view class="weui-cell__ft weui-cell__ft_in-access">{{healthy.inspectTime}}</view>
</view>
<view class="weui-cell weui-cell_access">
  <view class="weui-cell__bd">体检机构</view>
  <view class="weui-cell__ft weui-cell__ft_in-access">{{healthy.inspectMechanism}}</view>
</view>
<view class="weui-cell weui-cell_access">
  <view class="weui-cell__bd">体检项目</view>
  <view class="weui-cell__ft weui-cell__ft_in-access">{{healthy.inspectProject}}</view>
</view>
<view class="weui-cell weui-cell_access">
  <view class="weui-cell__bd">体检结果</view>
  <view class="weui-cell__ft weui-cell__ft_in-access">{{healthy.inspectResult}}</view>
</view>
<view class="weui-cell weui-cell_access" style='border-bottom: none;'>
  <view class="weui-cell__bd">体检报告上传</view>
</view>
<view class='pian' wx:for="{{healthy.inspectImage}}">
  <image class='pian01' src='{{url}}/imgfile/{{item.url}}'></image>
</view>