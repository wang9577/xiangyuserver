/* pages/Myconsultant/index.wxss */
page{ background: #f5f5f5}
.Myco_pic,.Myco,.Myco_pic01,.bgg,.Myco01,.Myco_bt,.Myco_left,.fanwei{ float: left;}
.Myco_pic,.Myco,.bgg,.Myco01,.Myco_bt,.fanwei{ width: 100%;}
.fanwei,.Myco_pic{ position: relative}
.bgg{ position: absolute; top: 0}
.Myco{ position: relative;  z-index: 1;}
.Myco_pic,.bgg,.fanwei{ height: 400rpx;}
.Myco_pic{ filter: blur(4px);}
.fanwei{ overflow: hidden}
.Myco{ box-sizing: border-box;}
.Myco{ padding: 30rpx;}
.Myco{ background: #fff;}
.Myco_pic01{ border-radius: 50%; width: 130rpx; height: 130rpx; border: 6rpx solid #fff; margin-top: -94rpx;}
.bgg{ background: rgba(0, 0, 0, 0.3)}
.Myco01{ padding-left: 20rpx; margin-top: 10rpx;}
.weui-cell__bd {
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	flex: 1
}
.weui-cell__ft {
	text-align: right;
	color: #999
}
.weui-cell {
	padding: 15px 5px;
	position: relative;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	align-items: center;
  border-bottom: 2rpx solid #eee;
}
.weui-cell__ft_in-access {
	padding-right: 15px;
	position: relative
}
.weui-cell__ft_in-access:after {
	content: " ";
	display: inline-block;
	height: 6px;
	width: 6px;
	border-width: 4rpx 4rpx 0 0;
	border-color: #ccc;
	border-style: solid;
	-webkit-transform: matrix(.71, .71, -.71, .71, 0, 0);
	transform: matrix(.71, .71, -.71, .71, 0, 0);
	top: -2px;
	position: absolute;
	top: 50%;
	margin-top: -4px;
	right: 2px
}